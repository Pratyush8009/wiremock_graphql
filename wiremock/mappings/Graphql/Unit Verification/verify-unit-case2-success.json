{
    "request": {
        "method": "POST",
        "url": "/graphql",
        "customMatcher": {
            "name": "graphql-body-matcher",
            "parameters": {
                "query": "query GetUnit($unitId: ID!) { unit(id: $unitId) { unitId unitName description verified } }",
                "variables": {
                    "unitId": "UNIT_001",
                    "unitName": "Thermal_Unit"
                }
            }
        }
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        },
        "body": "{\n  \"data\": {\n    \"unit\": {\n      \"unitId\": \"UNIT_001\",\n      \"unitName\": \"Thermal Unit\",\n      \"description\": \"Main Thermal Unit\",\n      \"verified\": true\n    }\n  }\n}"
    }
}